        "dotenv": "^16.0.0",
        "electron": "16.0.8",
        "electron-builder": "^22.14.13",
        "eslint": "^8.10.0",
        "eslint-config-airbnb-base": "^15.0.0",
        "eslint-config-prettier": "^8.3.0",
        "eslint-import-resolver-typescript": "^2.5.0",
        "eslint-plugin-cypress": "^2.12.1",
